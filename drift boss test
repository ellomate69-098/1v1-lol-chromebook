        window.addEventListener('resize', resizePage);
        function resizePage() {
            var g = document.getElementById("game");
            // ASPECT: 3/2
            var s = 96/170;
            g.height = document.getElementById("game-col").clientWidth * s;
            g.width = document.getElementById("game-col").clientWidth;

            } resizePage();
